{
    "dateTime.showAMPM": true,
    "dateTime.showDayOfMonth": true,
    "dateTime.showDayOfWeek": true,
    "dateTime.showMonth": true,
    "dateTime.use24HourClock": false,

    "workbench.colorTheme": "One Dark Rain Coat Bright",
    "workbench.iconTheme": "vscode-great-icons",

    "beautify.config": "/Users/joaquinb/.jsbeautifyrc",
    "beautify.ignore": ["*/**.test.*", "*/**.spec.*", "**/test/**/*"],

    "bashBeautify.tabSize": 4,

    "npm.runInTerminal": true,
    "npm.packageManager": "yarn",

    "gitlens.historyExplorer.enabled": true,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },

    "sublimeImporter.hasPromptedOnStartup": true,

    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.wordWrap": "off",
    "editor.rulers": [100],
    "editor.wrappingIndent": "same",
    "editor.showFoldingControls": "mouseover",
    "editor.fontSize": 10,
    "editor.scrollBeyondLastLine": true,

    "terminal.external.linuxExec": "xterm",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.env.linux": {},
    "terminal.integrated.env.osx": {},
    "terminal.integrated.scrollback": 5000,
    "terminal.integrated.setLocaleVariables": true,
    "terminal.integrated.shell.linux": "/bin/zsh",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.rightClickBehavior": "copyPaste",
    "terminal.integrated.fontSize": 12,

    "yarn.runInTerminal": true,

    "eslint.autoFixOnSave": true,
    "eslint.packageManager": "yarn",
    "eslint.run": "onType",
    "eslint.provideLintTask": true,

    "previewjsdoc.port": 4002,

    "jshint.packageManager": "yarn",
    "jshint.options": {
        "eqnull": true,
        "esversion": 6,
        "funcscope": true,
        "futurehostile": true,
        "loopfunc": false,
        "maxerr": 50,
        "node": true,
        "shadow": true,
        "unused": true
    },

    "window.restoreFullscreen": true,
    "window.restoreWindows": "all",

    "files.autoSave": "onFocusChange",
    "files.trimTrailingWhitespace": true,
    "editor.minimap.enabled": false,

    "quicktask.useYarn": false,

    "vsnotes.defaultNotePath": "/Users/joaquinb/vsnotes",
    "vsnotes.ignorePatterns": [],

    "openInGitHub.gitHubDomain": "https://bitbucket.org/cattlebruisers",
    "openInGitHub.providerType": "bitbucket",

