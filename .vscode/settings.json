{
  "bashBeautify.tabSize": 4,
  "files.associations": {
    ".auditorrc": "jsonc",
    ".bash*": "bat",
    ".eslintrc.js": "javascript",
    ".gpmrc": "json",
    ".node-inspectorrc": "json",
    ".nycrc": "json",
    ".prettierrc": "json",
    ".sgcrc": "jsonc",
    ".tslintrc": "jsonc",
    ".zaliases": "shellscript",
    ".zsh_aliases": "shellscript",
    ".zsh_functions": "shellscript",
    ".zshrc": "shellscript",
    ".zshrc-antigen": "shellscript",
    "*.code-workspace": "jsonc",
    "*.log.*": "log",
    "*.phpi1": "php",
    "*.zsh": "shellscript",
    "configure": "shellscript",
    "docker-ngrok": "shellscript",
    "gitto*": "javascript",
    "Makefile.in": "makefile",
    "merge-simplecov": "ruby",
    "snykscan": "shellscript",
    "zsh_functions": "shellscript",
    "zshrc": "shellscript"
  },
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.babelrc*": false,
    "**/.DS_Store": true,
    "**/.eslintrc*": false,
    "**/.git": false,
    "**/.hg": true,
    "**/.jshintrc*": false,
    "**/.svn": true,
    "**/CVS": true
  },
  "files.trimTrailingWhitespace": true,
  "json.format.enable": true,
  "sortJSON.excludedFiles": [
    "package.json",
    "package-lock.json"
  ]
}